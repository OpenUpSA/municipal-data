{
    "fact_table": "aged_creditor_facts",
    "label": "Aged Creditor Analysis",
    "description": "Analysis of creditors aggregated by age of debt. Does not include individual creditors.",
    "return_forms": ["AC", "ACA"],
    "update_cycle": "quarter",
    "last_updated": "2020-10",
    "dimensions": {
        "demarcation": {
            "attributes": {
                "code": {
                    "column": "scorecard_geography.geo_code",
                    "label": "Demarcation Code",
                    "type": "string",
                    "description": "Municipality code assigned by the Municipal Demarcation Board"
                },
                "label": {
                    "column": "scorecard_geography.name",
                    "label": "Name",
                    "type": "string",
                    "description": ""
                }
            },
            "key_attribute": "code",
            "label_attribute": "label",
            "description": "Municipality demarcation details",
            "label": "Municipality",
            "join_column": "demarcation_code"
        },
        "item": {
            "attributes": {
                "code": {
                    "column": "aged_creditor_items.code",
                    "label": "Code",
                    "type": "string",
                    "description": "Code used by the treasury"
                },
                "label": {
                    "column": "aged_creditor_items.label",
                    "label": "Label",
                    "type": "string",
                    "description": ""
                },
                "position_in_return_form": {
                    "column": "aged_creditor_items.position_in_return_form",
                    "label": "Position in Return Form",
                    "type": "integer",
                    "description": "The position of this item in the Section 71 Return form - useful for sorting line items to be ordered as they are in the return form to group related items and place them below the section headings and above section totals."
                },
                "return_form_structure": {
                    "column": "aged_creditor_items.return_form_structure",
                    "label": "Return Form Structure",
                    "type": "string",
                    "description": "What structural role this line item has in the return form"
                },
                "composition": {
                    "column": "aged_creditor_items.composition",
                    "label": "Composition",
                    "type": "string",
                    "description": "The composition of this item. Indicates how this item is composed of other items in this cube for the same mnuicipality, financial year and period, and amount type using item codes."
                }
            },
            "key_attribute": "code",
            "label_attribute": "label",
            "description": "",
            "label": "Item",
            "join_column": "item_code",
            "non_aggregatable": true
        },
        "financial_year_end": {
            "attributes": {
                "year": {
                    "column": "financial_year",
                    "label": "Year End",
                    "type": "integer",
                    "description": ""
                }
            },
            "key_attribute": "year",
            "description": "The calendar year falling at the end of the financial year",
            "label": "Financial Year",
            "non_aggregatable": true
        },
        "period_length": {
            "attributes": {
                "length": {
                    "column": "period_length",
                    "label": "Length",
                    "type": "string",
                    "description": ""
                }
            },
            "key_attribute": "length",
            "description": "The length of the period this figure corresponds to",
            "label": "Period Length",
            "non_aggregatable": true
        },
        "financial_period": {
            "attributes": {
                "period": {
                    "column": "financial_period",
                    "label": "Period",
                    "type": "integer",
                    "description": ""
                }
            },
            "key_attribute": "period",
            "description": "The period this figure corresponds to, e.g. the month number of a month within the financial year, or the calendar year end of a financial year.",
            "label": "Financial Period"
        },
        "amount_type": {
            "attributes": {
                "code": {
                    "column": "amount_type.code",
                    "label": "Code",
                    "type": "string",
                    "description": ""
                },
                "label": {
                    "column": "amount_type.label",
                    "label": "Label",
                    "type": "string",
                    "description": ""
                }
            },
            "key_attribute": "code",
            "description": "Whether this figure is actual, budgeted, audited etc.",
            "label_attribute": "label",
            "label": "Amount Type",
            "join_column": "amount_type_code",
            "non_aggregatable": true
        }
    },
    "measures": {
        "g1_amount": {
            "column": "g1_amount",
            "label": "Over 1 Year"
        },
        "l1_amount": {
            "column": "l1_amount",
            "label": "181 Days - 1 Year"
        },
        "l120_amount": {
            "column": "l120_amount",
            "label": "91 - 120 Days"
        },
        "l150_amount": {
            "column": "l150_amount",
            "label": "121 - 150 Days"
        },
        "l180_amount": {
            "column": "l180_amount",
            "label": "151 - 180 Days"
        },
        "l30_amount": {
            "column": "l30_amount",
            "label": "30 Days And Below"
        },
        "l60_amount": {
            "column": "l60_amount",
            "label": "31 - 60 Days"
        },
        "l90_amount": {
            "column": "l90_amount",
            "label": "61 - 90 Days"
        },
        "total_amount": {
            "column": "total_amount",
            "label": "Total"
        }
    }
}
